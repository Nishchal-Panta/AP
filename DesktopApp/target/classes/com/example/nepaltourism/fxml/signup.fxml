<?xml version="1.0" encoding="UTF-8"?>
<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.nepaltourism.desktopapp.controllers.SignupController" prefWidth="400" prefHeight="400">
<Label fx:id="nameLabel" text="Name" AnchorPane.topAnchor="20" AnchorPane.leftAnchor="50"/>
<TextField fx:id="nameField" prefWidth="300" AnchorPane.topAnchor="50" AnchorPane.leftAnchor="50"/>
<Label fx:id="contactLabel" text="Contact" AnchorPane.topAnchor="90" AnchorPane.leftAnchor="50"/>
<TextField fx:id="contactField" prefWidth="300" AnchorPane.topAnchor="120" AnchorPane.leftAnchor="50"/>
<Label fx:id="nationalityLabel" text="Nationality" AnchorPane.topAnchor="160" AnchorPane.leftAnchor="50"/>
<TextField fx:id="nationalityField" prefWidth="300" AnchorPane.topAnchor="190" AnchorPane.leftAnchor="50"/>
<Label fx:id="emergencyContactLabel" text="Emergency Contact" AnchorPane.topAnchor="230" AnchorPane.leftAnchor="50"/>
<TextField fx:id="emergencyContactField" prefWidth="300" AnchorPane.topAnchor="260" AnchorPane.leftAnchor="50"/>
<Label fx:id="passwordLabel" text="Password" AnchorPane.topAnchor="300" AnchorPane.leftAnchor="50"/>
<PasswordField fx:id="passwordField" prefWidth="300" AnchorPane.topAnchor="330" AnchorPane.leftAnchor="50"/>
<HBox spacing="10" AnchorPane.topAnchor="370" AnchorPane.leftAnchor="100" AnchorPane.rightAnchor="100">
    <Button text="Sign Up" onAction="#handleSignup"/>
    <Button text="Back" onAction="#handleBack"/>
</HBox>
</AnchorPane>

